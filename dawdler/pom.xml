			<dependency>
				<groupId>dawdler</groupId>
				<artifactId>dawdler-load-bean</artifactId>
				<version>${dawdler.build.version}</version>
			</dependency>
			<dependency>
				<groupId>dawdler</groupId>
				<artifactId>dawdler-client-plug-velocity</artifactId>
				<version>${dawdler.build.version}</version>
			</dependency>
			<dependency>
				<groupId>dawdler</groupId>
				<artifactId>dawdler-serialization</artifactId>
